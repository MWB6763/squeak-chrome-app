{
  "manifest_version": 2,
  "name": "SqueakJS",
  "version": "0.6",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "img/icon16.png",
    "128": "img/icon128.png"
  },
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "storage",
    "clipboardRead",
    "clipboardWrite"
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  }
}
